FUNCTION fn_dot_product(var v1: ARRAY_OF INTEGER,v2 : ARRAY_OF INTEGER,n:integer) : INTEGER
VAR
ps:INTEGER:=0;
i:INTEGER;
BEGIN
FOR i FROM 0 TO n  DO
ps:=ps+v1[i]*v2[i];
END_FOR
IF (fn_dot_product(v1,v2)=0) THEN
Write('the Two vectors are orthogonal');
ELSE
Write('the Two vectors are not orthogonal');
END_IF
RETURN ps
END
ALGORITHM dot_product_algo
VAR
n,i:INTEGER;
v1,v2 : ARRAY_OF INTEGER[n];
BEGIN
Read(n);
FOR i FROM 0 TO n-1  DO
Read(v1[i]);
Read(v2[i]);
END_FOR
fn_dot_product(v1,v2,n);
END