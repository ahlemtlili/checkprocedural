PROCEDURE dot_product(v1: ARRAY_OF INTEGER,v2 : ARRAY_OF INTEGER,VAR ps:integer)
VAR
i:INTEGER;
ps:INTEGER:=0;
BEGIN
FOR i FROM 0 TO n  DO
ps:=ps+v1[i]*v2[i];
END_FOR   
END
//PROGRAM PRINCIPAL
ALGORITHM dot_product_algo
VAR
n,i:INTEGER;
ps:INTEGER:=0;
v1,v2 : ARRAY_OF INTEGER[n];
BEGIN
Read(n);
FOR i FROM 1 TO n STEP step  DO
Read(v1);
Read(v2); 
dot_product(v1,v2,ps)
IF (ps=0) THEN
Write('the Two vectors are orthogonal');
ELSE
Write('the Two vectors are not orthogonal');
END_IF
END_FOR
END