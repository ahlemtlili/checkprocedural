PROCEDURE dot_product(VAR v1: ARRAY_OF INTEGER,v2 : ARRAY_OF INTEGER,n:integer)
VAR
    i:INTEGER;
    ps:INTEGER:=0;
BEGIN
    FOR i FROM 0 TO n  DO
        ps:=ps+v1[i]*v2[i];
    END_FOR
    RETURN ps
END
ALGORITHM dot_product_algo
VAR
n,i:INTEGER;
v1,v2 : ARRAY_OF INTEGER[n];
BEGIN
Read(n);
FOR i FROM 0 TO n-1  DO
Read(v1[i]);
Read(v2[i]);
END_FOR
IF (fn_dot_product(v1,v2,n)=0) THEN
Write('the Two vectors are orthogonal');
ELSE
Write('the Two vectors are not orthogonal');
END_IF
END