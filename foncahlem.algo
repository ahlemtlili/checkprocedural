PROCEDURE dot_product(VAR v1: ARRAY_OF INTEGER,v2 : ARRAY_OF INTEGER,n:integer)
VAR
    i:INTEGER;
BEGIN
    FOR i FROM 0 TO n  DO
        ps:=ps+v1[i]*v2[i];
    END_FOR
        IF (ps=0) THEN
        Write('Two vectors are orthogonal');
    ELSE
        Write('Two vectors are not orthogonal');
    END_IF
END
ALGORITHM dot_product_algo
VAR
ps:INTEGER:=0;
n,i:INTEGER;
v1,v2 : ARRAY_OF INTEGER[n];
BEGIN
Read(n);
FOR i FROM 0 TO n-1  DO
Read(v1[i]);
Read(v2[i]);
END_FOR
dot_product(v1,v2,n)
END